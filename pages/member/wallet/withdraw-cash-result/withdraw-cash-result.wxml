<view>
  <view class="result">
    <icon type="success" size="80"/>
    <text>提交成功</text>
    <view class="text">
    	<view>平台将在3-10个工作日处理</view>
      <view>您的提现申请，请耐心等待</view>
    </view>
    <view class="button">
      <navigator url="../../wallet/wallet" open-type="redirectTo">
        <button type="primary">确定</button>
      </navigator>
    </view>
    <view class="footer">
    	客服电话：<text bindtap="makePhone">{{phoneNumber}}</text> | <text bindtap="openAgreement">关于我们</text>
    </view>
  </view>
</view>